{
  "version": "1.0.7",
  "name": "jsonms/php",
  "description": "A JSON.ms requests handler to install on your own server.",
  "type": "library",
  "license": "MIT",
  "scripts": {
    "setup-hooks": "cp .pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
    "post-install-cmd": [
      "php install.php"
    ]
  },
  "autoload": {
    "psr-4": {
      "JSONms\\": "src/"
    }
  },
  "authors": [
    {
      "name": "JSON.ms"
    }
  ],
  "require": {
    "php": "^8.0",
    "james-heinrich/getid3": "^1.9"
  }
}
